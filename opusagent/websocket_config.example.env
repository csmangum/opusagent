# OpenAI API Configuration (Required)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-12-17

# WebSocket Connection Pool Settings
WEBSOCKET_MAX_CONNECTIONS=10
WEBSOCKET_MAX_CONNECTION_AGE=3600
WEBSOCKET_MAX_IDLE_TIME=300
WEBSOCKET_HEALTH_CHECK_INTERVAL=30
WEBSOCKET_MAX_SESSIONS_PER_CONNECTION=10

# WebSocket Connection Parameters
WEBSOCKET_PING_INTERVAL=20
WEBSOCKET_PING_TIMEOUT=30
WEBSOCKET_CLOSE_TIMEOUT=10

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Logging Configuration
LOG_LEVEL=DEBUG 